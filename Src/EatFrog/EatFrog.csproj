<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <Version>1.0.1</Version>
        <TargetFramework>net8.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>disable</Nullable>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <Authors>furesoft</Authors>
        <Description>A framework to build emitters for compilers</Description>
        <RepositoryUrl>https://github.com/furesoft/EatFrog</RepositoryUrl>
        <PackageTags>compiler</PackageTags>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="Syroot.BinaryData" Version="5.2.2" />
      <PackageReference Include="Furesoft.PrattParser" Version="1.0.59" />
    </ItemGroup>

</Project>
